<?xml version="1.0" encoding="UTF-8"?>

<web-app>
    <servlet>
        <servlet-name>Hello</servlet-name>
        <servlet-class>serve.AddServlet</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>Hello</servlet-name>
        <url-pattern>/add</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>anagram</servlet-name>
        <servlet-class>serve.CheckAnagram</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>anagram</servlet-name>
        <url-pattern>/anagram</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>cookie</servlet-name>
        <servlet-class>cookie.SaveCookie</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>cookie</servlet-name>
        <url-pattern>/savecookie</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>showcookie</servlet-name>
        <servlet-class>cookie.ShowCookies</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>showcookie</servlet-name>
        <url-pattern>/showcookie</url-pattern>
    </servlet-mapping>
    
    
    <servlet>
        <servlet-name>session</servlet-name>
        <servlet-class>session.AddSession</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>session</servlet-name>
        <url-pattern>addsession</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>showsession</servlet-name>
        <servlet-class>session.ShowSession</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>showsession</servlet-name>
        <url-pattern>showsessio</url-pattern>
    </servlet-mapping>
</web-app>